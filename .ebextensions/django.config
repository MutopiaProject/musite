  01_migrate:
    command: "python manage.py migrate"
    leader_only: true

  02_collectstatic:
    command: "python manage.py collectstatic --noinput"
    leader_only: true

option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: musite/wsgi.py

packages:
  yum:
    postgresql95-devel: []
